{
  "name": "nextchat",
  "private": false,
  "license": "mit",
  "scripts": {
    "mask": "npx tsx app/masks/build.ts",
    "mask:watch": "npx watch \"npx tsx app/masks/build.ts\" app/masks",
    "dev": "concurrently -r \"pnpm run mask:watch\" \"next dev --turbo\"",
    "build": "pnpm run mask && cross-env BUILD_MODE=standalone next build --turbo",
    "start": "next start",
    "lint": "next lint",
    "export": "pnpm run mask && cross-env BUILD_MODE=export BUILD_APP=1 next build",
    "export:dev": "concurrently -r \"pnpm run mask:watch\"  \"cross-env BUILD_MODE=export BUILD_APP=1 next dev\"",
    "app:dev": "concurrently -r \"pnpm run mask:watch\" \"pnpm tauri dev\"",
    "app:build": "pnpm run mask && pnpm tauri build",
    "app:clear": "pnpm tauri dev",
    "prompts": "node ./scripts/fetch-prompts.mjs",
    "prepare": "husky install",
    "proxy-dev": "sh ./scripts/init-proxy.sh && proxychains -f ./scripts/proxychains.conf pnpm run dev",
    "test": "node --no-warnings --experimental-vm-modules $(pnpm bin jest) --watch",
    "test:ci": "node --no-warnings --experimental-vm-modules $(pnpm bin jest) --ci"
  },
